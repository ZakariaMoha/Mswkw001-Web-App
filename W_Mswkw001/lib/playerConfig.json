{
  "manifestUrl": "https://example.com/manifest.mpd",
  "playerConfig": {
    "initialVideoBitrate": 500000,
    "initialAudioBitrate": 128000,
    "maxVideoBitrate": 2000000,
    "maxAudioBitrate": 256000,
    "preferredAudioTracks": [],
    "preferredTextTracks": [],
    "preferredVideoTracks": [],
    "wantedBufferAhead": 30,
    "maxBufferAhead": 60,
    "maxBufferBehind": 30,
    "throttleVideoBitrateWhenHidden": true,
    "enableFastSwitching": true
  },
  "drmConfig": {
    "widevine": {
      "serverCertificateUrl": "https://example.com/widevine-cert",
      "licenseServerUrl": "https://example.com/widevine-license",
      "httpRequestHeaders": {
        "Authorization": "Bearer token"
      }
    },
    "playready": {
      "licenseServerUrl": "https://example.com/playready-license",
      "httpRequestHeaders": {
        "Authorization": "Bearer token"
      }
    }
  }
}
